{
  "version": 2,
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 60,
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}